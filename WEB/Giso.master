    <style type="text/css">
        ._ASC{background:url(../../images/sort_incr.png) no-repeat;background-position:center left;padding-left:20px !important;}
        ._DESC{background:url(../../images/sort_decrease.png) no-repeat;background-position:center left;padding-left:20px !important;}
        .sort{
            cursor:pointer;
        }
        .sort:after {
            float: right;
            display: inline;
            content: "\f0dc";
            font-family: FontAwesome;
            font-size: 13px;
            font-weight: 400;
            color: #555;
            cursor:pointer;
        } 
        .ASC:after {
            float: right;
            display: inline;
            content: "\f0dd";
            font-family: FontAwesome;
            font-size: 13px;
            font-weight: 400;
            color: #55f;
            cursor:pointer;
        }
        .DESC:after {
            float: right;
            display: inline;
            content: "\f0de";
            font-family: FontAwesome;
            font-size: 13px;
            font-weight: 400;
            color: #55f;
            cursor:pointer;
        }

        .NoData { text-shadow:#fff 0px 1px 0, #000 0 -1px 0;}

